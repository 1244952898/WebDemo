<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>/TestServlet</servlet-name>
        <servlet-class>Upload.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>/TestServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FileServlet</servlet-name>
        <servlet-class>Upload.FileUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileServlet</servlet-name>
        <url-pattern>/fileupload</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FileServlet1</servlet-name>
        <servlet-class>Upload.FileUpload1Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileServlet1</servlet-name>
        <url-pattern>/fileupload1</url-pattern>
    </servlet-mapping>
</web-app>